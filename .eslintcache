essJWT\") && dispatch(loginSuccess());\r\n  }, [dispatch, isAuth, user._id]);\r\n\r\n  return (\r\n    <Route\r\n      {...rest}\r\n      render={() =>\r\n        isAuth ? <DefaultLayout>{children}</DefaultLayout> : <Redirect to=\"/\" />\r\n      }\r\n    />\r\n  );\r\n};\r\n","C:\\Users\\Lenovo\\code\\studyroof\\studyrooCrm-frontend\\src\\pages\\addTask\\Addtask.page.js",["226","227","228","229"],"import React,{useState} from 'react'\r\nimport \"./addTask.style.css\";\r\nimport {useDispatch, useSelector} from \"react-redux\"\r\nimport {addTaskPending,addTaskSuccess,addTaskError} from \"./addTaskSlice\"\r\nimport {\r\n  Spinner,Alert\r\n} from \"react-bootstrap\";\r\nimport {createNewTask} from \"../../api/taskApi\"\r\n\r\n\r\nexport const Addtask = () => {\r\n\r\n    const dispatch=useDispatch()\r\n \r\n    const [taskName, setTaskName] = useState(\"\");\r\n    const [type, setType] = useState(\"\");\r\n    const [dueDate, setDueDate] = useState(\"\");\r\n    const [studentAssign, setStudentAssign] = useState(\"\");\r\n    const [assignTo, setAssignTo] = useState(\"\");\r\n\r\n    const [userGroup, setUserGroup] = useState(\"\");\r\n    const [offices, setOffices] = useState(\"\");\r\n      \r\n    \r\n      const handleOnChange = (e) => {\r\n        const { name, value } = e.target;\r\n    \r\n        switch (name) {\r\n          case \"taskName\":\r\n            setTaskName(value);\r\n            break;\r\n    \r\n            case \"type\":\r\n                setType(value);\r\n            break;\r\n    \r\n            case \"dueDate\":\r\n            setDueDate(value);\r\n            break;\r\n    \r\n           \r\n            case \"studentAssign\":\r\n            setStudentAssign(value);\r\n            break;\r\n    \r\n            case \"assignTo\":\r\n                setAssignTo(value);\r\n                break;\r\n        \r\n            case \"userGroup\":\r\n              setUserGroup(value);\r\n              break;\r\n    \r\n              case \"office